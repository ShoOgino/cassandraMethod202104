    static void writeHeader(ByteBuffer frame, boolean isSelfContained, int dataLength)
    {
        int header3b = dataLength;
        if (isSelfContained)
            header3b |= 1 << 17;
        int crc = crc24(header3b, 3);
        put3b(frame, 0, header3b);
        put3b(frame, 3, crc);
    }

